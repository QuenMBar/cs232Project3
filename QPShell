  bool notExit;
  int childArgc;
  char **childArgv;

  Prompt *myPromptPoint;

  void printWorkDir(Prompt myPrompt);
  void exitShell();
  void changeDir(CommandLine CMD);
  void runCommand(CommandLine CMD, string command);
  void parseCommand(CommandLine CMD, Prompt myPrompt);
};

